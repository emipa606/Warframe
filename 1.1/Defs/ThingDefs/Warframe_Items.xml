<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ResourceBase">
    <defName>WFItem_OrokinCell</defName>
    <label>orokin cell</label>
    <description>Advanced spacer tech structural material. Plasteel is extremely strong due to its unique molecular structure.</description>
    <graphicData>
      <texPath>Items/OrokinCell</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <useHitPoints>false</useHitPoints>
    <statBases>
      <MarketValue>4000</MarketValue>
      <Mass>1.14514</Mass>
	  <SellPriceFactor>0.1</SellPriceFactor>
    </statBases>
    <thingCategories>
      <li>Items</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <thingSetMakerTags>
      <li>RewardSpecial</li>
    </thingSetMakerTags>
  </ThingDef>
  
  

 <ThingDef ParentName="ResourceBase">
    <defName>Warframe_Item</defName>
    <label>Warframe Item</label>
    <description>It contains A Warframe.</description>
    <graphicData>
      <texPath>WOBJ/VoidPlace</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tradeability>None</tradeability>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>1</stackLimit>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <Mass>0.6</Mass>
      <Flammability>1.0</Flammability>
			<DeteriorationRate>2.0</DeteriorationRate>
		</statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Usable">
        <useJob>UseItem</useJob>
        <useLabel>Open</useLabel>
      </li>
      <li Class="CompProperties_UseEffect">
        <compClass>Warframe.CompUseEffect_WarframeItem</compClass>
      </li>
      <li Class="CompProperties_UseEffect">
        <compClass>CompUseEffect_DestroySelf</compClass>
      </li>
    </comps>
  </ThingDef>

  
  

  
</Defs>
