<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef Name="WarframeBodyPartRecipeBase" Abstract="True">
    <description>Make a warframe bodypart.</description>
    <jobString>Making warframe bodypart.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>3000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <researchPrerequisite>WarframeCrafter</researchPrerequisite>
  </RecipeDef>
  <!--Excalibur-->
  <RecipeDef ParentName="WarframeBodyPartRecipeBase">
    <defName>Make_Excalibur_Head</defName>
    <label>make excalibur head</label>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>150</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Gold</li>
        <li>ComponentIndustrial</li>
        <li>Plasteel</li>
        <li>Silver</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <WFPart_Excalibur_Head>1</WFPart_Excalibur_Head>
    </products>
  </RecipeDef>
