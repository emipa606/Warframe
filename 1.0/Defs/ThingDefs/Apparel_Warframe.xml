<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef Name="WarframeApparelBase" Abstract="True">
    <thingClass>Apparel</thingClass>
    <category>Item</category>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <useHitPoints>false</useHitPoints>
    <techLevel>Ultra</techLevel>
	<menuHidden>true</menuHidden>
    <graphicData>
      <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
    </graphicData>
    <drawGUIOverlay>true</drawGUIOverlay>
    <statBases>
      <DeteriorationRate>0</DeteriorationRate>
      <Beauty>0</Beauty>
    </statBases>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>True</alwaysHaulable>
    <tickerType>Never</tickerType>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li>
        <compClass>CompQuality</compClass>
      </li>
    </comps>
    <tradeability>None</tradeability>
    <destroyOnDrop>true</destroyOnDrop>
    <smeltable>false</smeltable>
    <generateCommonality>0</generateCommonality>	
  </ThingDef>
  

  
  
<!-- Excalibur-->
  
  <ThingDef ParentName="WarframeApparelBase">
    <defName>WF_Excalibur_Belt</defName>
    <label>Excalibur Belt</label>
    <description>The latest generation of shield of Polarisbloc has not been produced, only for important personnel.\n\nThis shield will counterattack the attackers when they are attacked.Although the counterattack has the time of cooling, it still needs to be careful because the counter attack is very accurate.</description>
    <thingClass>Warframe.WarframeBelt</thingClass>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/ShieldBelt/ShieldBelt</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tickerType>Normal</tickerType>
    <statBases>
      <Mass>2.4</Mass>
      <EnergyShieldRechargeRate>0.13</EnergyShieldRechargeRate>
      <EnergyShieldEnergyMax>1.1</EnergyShieldEnergyMax>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
	<comps>
	  <li Class="Warframe.CompProperties_HPSP">
        <compClass>Warframe.CompHPSP</compClass>
        <HP>100</HP>
		<SP>100</SP>
	  </li>
	</comps>
    <apparel>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <bodyPartGroups>
        <li>Waist</li>
      </bodyPartGroups>
      <layers>
        <li>Belt</li>
      </layers>
      <tags>
        <li>WFEXcaliburBelt</li>
      </tags>
    </apparel>
  </ThingDef>
  
  <ThingDef ParentName="WarframeApparelBase">
    <defName>WF_Excalibur_Armor</defName>
    <label>Excalibur Armor</label>
    <description>The standard armor for the security force provides a small amount of defense and moving.</description>
    <thingClass>Warframe.WarframeArmor</thingClass>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <techLevel>Ultra</techLevel>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/PowerArmor/PowerArmor</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tickerType>Normal</tickerType>
	<comps>
	  <li Class="Warframe.CompProperties_WarframeSkill">
        <compClass>Warframe.CompWarframeSkill</compClass>
        <mana1>25</mana1>
		<mana2>50</mana2>
		<mana3>75</mana3>
		<mana4>25</mana4>
	  </li>
	</comps>
    <statBases>
      <Mass>8</Mass>
      <ArmorRating_Blunt>0.18</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.20</ArmorRating_Sharp>
      <ArmorRating_Heat>0.2</ArmorRating_Heat>
      <Insulation_Cold>45</Insulation_Cold>
      <Insulation_Heat>45</Insulation_Heat>
      <Beauty>0</Beauty>
      <EquipDelay>8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>1</MoveSpeed>
      <Flammability>-0.25</Flammability>
      <MeleeDodgeChance>2</MeleeDodgeChance>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Neck</li>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Excalibur/ExcaliburArmor</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
        <li>Shell</li>
      </layers>
      <tags>
        <li>WFEXcaliburArmor</li>
      </tags>
    </apparel>
  </ThingDef>

  <ThingDef ParentName="WarframeApparelBase">
    <defName>WF_Excalibur_Head</defName>
    <label>Excalibur Head</label>
    <thingClass>Warframe.WarframeHead</thingClass>
    <description>The universal tactical goggles produced by Polarisbloc can effectively enhance the sight ability.</description>
    <tickerType>Normal</tickerType>
    <thingCategories>
      <li>Headgear</li>
    </thingCategories>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/CowboyHat/CowboyHat</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <Mass>1.7</Mass>
      <ArmorRating_Blunt>0.35</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.55</ArmorRating_Sharp>
      <ArmorRating_Heat>0.5</ArmorRating_Heat>
      <Insulation_Cold>-10</Insulation_Cold>
      <Insulation_Heat>10</Insulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <AimingDelayFactor>-0.1</AimingDelayFactor>
      <ShootingAccuracyPawn>1</ShootingAccuracyPawn>
      <MeleeDodgeChance>0.5</MeleeDodgeChance>
      <MeleeHitChance>0.5</MeleeHitChance>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Neck</li>
        <li>FullHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Excalibur/ExcaliburHead</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>WFEXcaliburHead</li>
      </tags>
      <hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
    </apparel>
  </ThingDef>
  
